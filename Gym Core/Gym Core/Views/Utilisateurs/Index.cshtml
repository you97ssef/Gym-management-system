@model IEnumerable<Gym_Core.Models.Utilisateur>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MotdepasseUtilisateur)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MotdepasseUtilisateur)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NomUtilisateur">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NomUtilisateur">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NomUtilisateur">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
